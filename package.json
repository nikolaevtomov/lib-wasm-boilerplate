{
  "name": "lib-wasm-boilerplate",
  "version": "0.0.4",
  "description": "Npm package boilerplate for wasm projects",
  "main": "dist/index.js",
  "scripts": {
    "dist": "rm -rf dist/ && mkdir -p dist/ && tsc --project './tsconfig.dist.json' && npm run dist:copy",
    "dist:copy": "cp -R ./lib/main.wasm ./dist",
    "start": "cross-env NODE_ENV=development DEBUG=true webpack-dev-server --mode development",
    "prettier": "prettier --write '**/*.{ts,tsx,js,jsx,css,less,json}'",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "lint": "tsc --noEmit && eslint 'lib/*.{ts,tsx,js,jsx,css,less,json}' --quiet --fix",
    "test": "mocha --require ts-node/register 'lib/**/*.spec.{ts,tsx,js,jsx}'"
  },
  "dependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "babel-loader": "^8.1.0",
    "babel-plugin-const-enum": "0.0.5",
    "core-js": "^3.6.5",
    "raf": "^3.4.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "autoprefixer": "^9.8.5",
    "chai": "^4.2.0",
    "circular-dependency-plugin": "^5.2.0",
    "compression-webpack-plugin": "^3.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^7.2.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.2.1",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "url": "git@github.com:nikolaevtomov/lib-wasm-boilerplate.git"
  }
}
